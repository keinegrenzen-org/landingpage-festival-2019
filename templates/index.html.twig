    <span
      class="sr-only"
      itemprop="image"
    >{{ image }}</span>
    {% block javascripts %}
      {{ encore_entry_script_tags('theme', entrypointName = buildType) }}

      {% set files = encore_entry_js_files('soundcloud', entrypointName = buildType) %}
      {% for file in files %}
        <span
          class="soundcloud-entry"
          data-source="{{ file }}"
        ></span>
      {% endfor %}
