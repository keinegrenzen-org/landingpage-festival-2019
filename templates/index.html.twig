    {% include 'partials/footer.html.twig' %}

    <div class="soundcloud">
      <h4 class="title">Musik H&ouml;ren</h4>
      <div
        class="soundcloud-embed"
        data-source="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/playlists/1143840418%3Fsecret_token%3Ds-1wmotc2KF3I&color=%23fc2d2d&auto_play=false&hide_related=false&show_comments=false&show_user=false&show_reposts=false&show_artwork=false&download=false&sharing=false&buying=false"
