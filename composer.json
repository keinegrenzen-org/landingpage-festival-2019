{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^8.1",
    "ext-curl": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "barthy-koeln/browserslist-check-bundle": "^1.0",
    "symfony/asset": "*",
    "symfony/console": "*",
    "symfony/debug-bundle": "*",
    "symfony/dotenv": "*",
    "symfony/finder": "*",
    "symfony/flex": "^1.18",
    "symfony/framework-bundle": "*",
    "symfony/monolog-bundle": "^3.7",
    "symfony/runtime": "*",
    "symfony/twig-bundle": "*",
    "symfony/webpack-encore-bundle": "^1.16",
    "symfony/yaml": "*"
  },
  "require-dev": {
    "symfony/stopwatch": "*",
    "symfony/web-profiler-bundle": "*"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "replace": {
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "^6.2"
    }
  }
}
